{
    "manifest_version": 3,
    "name": "Extension Vue3 Template",
    "homepage_url": "http://localhost:8080/",
    "description": "Extension Vue3 Template",
    "default_locale": "en",
    "permissions": [
        "storage",
        "background"
    ],
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "background": {
        "service_worker": "js/background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [
                "css/content-script.0cafa637.css"
            ],
            "js": [
                "js/content-script.js"
            ],
            "run_at": "document_end"
        }
    ],
    "action": {
        "default_title": "",
        "default_icon": {
            "19": "icons/19.png",
            "38": "icons/38.png"
        }
    },
    "version": "1.0.0"
}